    "cordova-sqlite-storage": "^3.4.0",
    "firebase": "^6.6.2",
    "ionic-angular": "3.9.5",
    "ionicons": "3.0.0",
    "moment": "^2.24.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.5.3",
    "sw-toolbox": "3.6.0",
    "sweetalert": "^2.1.2",
    "zone.js": "0.8.29"
